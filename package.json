{
    "name": "vscode-scope",
    "displayName": "Scope",
    "description": "Providing syntax highlighting for scope/usql scripts & views",
    "version": "0.0.5",
    "publisher": "vicey",
    "scripts": {
        "package": "vsce package"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "scope",
                "aliases": [
                    "Scope",
                    "scope"
                ],
                "extensions": [
                    ".script",
                    ".view",
                    ".usql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "scope",
                "scopeName": "source.scope",
                "path": "./syntaxes/scope.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "vsce": "^1.35.0"
    }
}
